{
  "name": "asleepace.com",
  "description": "a collection of me by me.",
  "license": "MIT",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "bunx --bun astro dev",
    "start": "bunx --bun astro build --port 4000 && bunx --bun astro preview --port 4000",
    "build": "bunx --bun astro build",
    "preview": "bunx --bun astro preview",
    "build:tailwind": "npx tailwindcss -i ./src/styles/global.css -o ./src/styles/output.css --watch",
    "deploy": "./scripts/deploy.sh",
    "astro": "bunx --bun astro"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/mdx": "4.0.3",
    "@astrojs/node": "9.0.0",
    "@astrojs/react": "4.1.2",
    "@astrojs/rss": "4.0.10",
    "@astrojs/sitemap": "3.2.1",
    "@astrojs/tailwind": "5.1.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "astro": "5.1.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.469.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.5.3"
  },
  "devDependencies": {
    "@types/bun": "^1.1.6"
  },
  "baseUrl": ".",
  "paths": {
    "@/*": [
      "./src/*"
    ],
    "@components/*": [
      "./src/components/*"
    ],
    "@content/*": [
      "./src/content/*"
    ],
    "@layouts/*": [
      "./src/layouts/*"
    ],
    "@pages/*": [
      "./src/pages/*"
    ],
    "@styles/*": [
      "./src/styles/*"
    ],
    "@constants": [
      "./src/consts.ts"
    ]
  }
}
