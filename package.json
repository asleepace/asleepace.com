{
  "name": "asleepace.com",
  "description": "a collection of me by me.",
  "license": "MIT",
  "type": "module",
  "version": "1.1.1",
  "scripts": {
    "check": "bun --bun astro check",
    "dev": "bunx --bun astro dev",
    "start": "bunx --bun astro build --host 0.0.0.0 --port 4321 && bunx --bun astro preview --host 0.0.0.0 --port 4321",
    "build": "bunx --bun astro build",
    "build:debug": "DEBUG=astro:*,vite:* bunx --bun astro build --verbose",
    "preview": "bunx --bun astro preview --host 0.0.0.0 --port 4321",
    "astro:upgrade": "npx @astrojs/upgrade",
    "build:tailwind": "npx @tailwindcss/cli -i ./src/styles/global.css -o ./src/styles/output.css",
    "watch:tailwind": "npx @tailwindcss/cli -i ./src/styles/global.css -o ./src/styles/output.css --watch",
    "deploy": "./scripts/deploy.sh",
    "util:upgrade": "bun upgrade",
    "util:check": "bunx --bun astro check",
    "run:server": "bun ./dist/server/entry.mjs"
  },
  "dependencies": {
    "@asleepace/exception": "^0.99.5",
    "@asleepace/mutex": "^1.0.2",
    "@asleepace/try": "^0.2.1",
    "@astrojs/check": "^0.9.5",
    "@astrojs/db": "^0.15.1",
    "@astrojs/mdx": "^4.3.9",
    "@astrojs/node": "9.3.0",
    "@astrojs/react": "4.3.0",
    "@astrojs/rss": "^4.0.13",
    "@astrojs/sitemap": "3.4.1",
    "@nanostores/react": "^0.8.4",
    "@tailwindcss/vite": "^4.1.16",
    "@tanstack/react-table": "^8.21.3",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript/sandbox": "^0.1.10",
    "astro": "5.11.0",
    "canvas-confetti": "^1.9.4",
    "chalk": "^5.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.525.0",
    "puppeteer": "^24.31.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/bun": "^1.3.1",
    "@types/canvas-confetti": "^1.9.0",
    "tw-animate-css": "^1.4.0"
  }
}
